<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">


<struts>
    <include file="struts-default.xml"/>
    <package name="library" extends="struts-default">
        <action name="LibraryOperations_*" method="{1}"  class="library.LibraryOperations">
        <result name="update">Update.jsp</result>
        <result name="bookdetail">BookInfo.jsp</result>
        <result name="delete">index.jsp</result>
        <result name="searchresult">SearchResult.jsp</result>
        <result name="allbooks">AllBooks.jsp</result>
        <result name="update_ok">index.jsp</result>
        <result name="error">Error.jsp</result>
        </action>
         
    </package>
</struts>